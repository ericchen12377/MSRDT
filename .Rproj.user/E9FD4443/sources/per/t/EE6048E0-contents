pdf("G:/My Drive/research topics/2017 SRDT & warranty/2020JQT/manuscript/figures/Comp_Pareto_prior.pdf",
    width=10, height=8)
par(family='serif')
par(mar = c(8, 6, 2, 2)+0.5)
counts_prior_PF1 <- c(nrow(tmp_PF1_MS1_100585_1),
                      nrow(tmp_PF1_MS1_100585_3),
                      nrow(tmp_PF1_MS1_050590_2),
                      nrow(tmp_PF1_MS1_050590_4),
                      #nrow(tmp_PF1_MS1_252595_6),
                      nrow(tmp_PF1_MS1_200575_05),
                      nrow(tmp_PF1_MS1_200575_2),
                      #nrow(tmp_PF1_MS1_301060_4),
                      nrow(tmp_PF1_MS1_noninfo)
)

p <- barplot(counts_prior_PF1,ylab="Number of Pareto Front",
             space = c(1,0.5,2,0.5,2,0.5,2),
             ylim = c(0, 100000), xlim = c(1,18),
             density = c(100,20,100,20,100,20,100),
             #border = c(3,3,3,3,3,2,2,2,1),
             cex.lab = 2, cex.axis = 1.5,
             col = c(1),
             xaxt="n",family='serif')
text(c(3,8,13,18), y = -1,
     srt = 30, adj = 1.1, xpd = TRUE,

     labels = c("Supportive", "More supportive",
                "Less Supportive", "Non informative"),
     cex=2,family='serif')
legend('top',legend = c('High variance','Low variance'),col = c(1,1), fill = c(1,1), density = c(100,20), bty = 'n',cex = 2)

dev.off()




pdf("G:/My Drive/research topics/2017 SRDT & warranty/2020JQT/manuscript/figures/Truncation_Pareto_real_ratio.pdf",
    width=10, height=8)
par(family='serif')
par(mgp=c(4,1.5,0))
par(mar = c(6, 7, 6, 6))
gray_seq <- c(0.2,0.5,0.9)
plot(CR_seq, PF_ratio_CR, ylab = "Selection ratio", xlab = "Risk", type = "l",
     col = gray.colors(1,start = gray_seq[1],end=gray_seq[1]), lwd = 5, lty = 4,
     cex.lab = 2, cex.axis = 2)
lines(PR_seq, PF_ratio_PR,
      col = gray.colors(1,start = gray_seq[2],end=gray_seq[2]),lwd = 5, lty = 4)
lines(RP_seq, PF_ratio_RP,
      col = gray.colors(1,start = gray_seq[3],end=gray_seq[3]),lwd = 5, lty = 4)
lines(CR_seq, PF_ratio_n,
      col = 1,lwd = 3, lty = 1)
lines(CR_seq, PF_ratio_W,
      col = gray.colors(1,start = gray_seq[2],end=gray_seq[2]),lwd = 5, lty = 3)
axis(3, at = c(0,0.2,0.4,0.6,0.8,1), labels = seq(0.08, 0.16, length.out = 6),
     cex.lab = 2, cex.axis = 2)
mtext("Warranty cost ratio", side=3, cex=2,las=1, line = 3)
axis(4, at = c(0,0.2,0.4,0.6,0.8,1), labels = c(0,20,40,60,80,100),las = 2,
     cex.lab = 2, cex.axis = 2)
mtext("n", side=4, cex=2,las=1, line = 3)

legend('topleft',legend = c('CR',
                            'PR',
                            'RP',
                            'n',
                            'W'),
       col = c(gray.colors(1,start = gray_seq[1],end=gray_seq[1]),
               gray.colors(1,start = gray_seq[2],end=gray_seq[2]),
               gray.colors(1,start = gray_seq[3],end=gray_seq[3]),
               'black',
               gray.colors(1,start = gray_seq[2],end=gray_seq[2])),
       bty = 'n', lty = c(4,4,4,1,3), lwd = c(5,5,5,5,5),cex = 1.5)
dev.off()








































