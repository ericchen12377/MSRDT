####Trade-off patterns
######## set up datasets
library(plot3D)

set.seed(10)
tmp <- as.data.frame(MS1_real)
tmp$Wc <- W_real$W
tmp <- tmp[tmp$n<=100,]

#tmp <- tmp[sample(1:nrow(tmp),20000),]
tmp[,'R1'] <- as.numeric(as.character(tmp[,'R1']))
tmp[,'R2'] <- as.numeric(as.character(tmp[,'R2']))
tmp[,'c1'] <- as.numeric(as.character(tmp[,'c1']))
tmp[,'c2'] <- as.numeric(as.character(tmp[,'c2']))
tmp[,'Wc'] <- as.numeric(as.character(tmp[,'Wc']))
tmp$RP <- 1 - tmp$AP
tmp$W <- tmp$Wc

tmp.R2 <- tmp[tmp[,'R2']==0.82,]
tmp.c1 <- tmp[tmp[,'c1']==0,]
tmp.R2.c1 <- tmp.R2[tmp.R2[,'c1']==0,]
tmp.c1.c2 <- tmp.c1[tmp.c1[,'c2']==0,]
tmp.c1.c2.n <- tmp.c1.c2[tmp.c1.c2[, 'n']<30,]

tmp.R1.R2 <- tmp.R2[tmp.R2[,'R1']==0.94,]
tmp.R1.R2.c1 <- tmp.R1.R2[tmp.R1.R2[,'c1']==0,]
tmp.R2.c1.c2 <- tmp.R2.c1[tmp.R2.c1[,'c2']==2,]



##General trade-off patterns
pdf("G:/My Drive/research topics/2017 SRDT & warranty/2020JQT/manuscript/figures/Tradeoff_real_CRPRAP_all.pdf",
    width=10, height=8)
par(family='serif')
par(mar = c(5, 5, 2, 2))
scatter3D(tmp[,'CR'],tmp[,'PR'],1-tmp[,'AP'],
          colvar = tmp[,'n'],
          cex=0.5,pch=19,col = gray.colors(10,start = 0.1,end=0.95),
          cex.lab = 2, cex.axis = 1.5, colkey = list(cex.clab = 2, cex.axis = 1.5),
          clab = 'n',xlab = 'CR', ylab = 'PR', zlab = 'RP',
          theta = -60, phi = 20)
dev.off()



#########Trade_off################################################################################################
cex.lab = 3
cex.axis = 2.5
cex.clab = 3
legend_cex = 2.5
c_1 <- expression(italic(c)[scriptscriptstyle(1)])
c_2 <- expression(italic(c)[scriptscriptstyle(2)])
c_12 <- expression(italic(c)[scriptscriptstyle(1)] + italic(c)[scriptscriptstyle(2)])

R_1 <- expression(italic(R)[scriptscriptstyle(italic(L1))])
R_2 <- expression(italic(R)[scriptscriptstyle(italic(L2))])
#CR vs PR fix R1,R2
pdf("G:/My Drive/research topics/2017 SRDT & warranty/2020JQT/manuscript/figures/Tradeoff_real_CRPR_c.pdf",
    width=10, height=8)
par(family='serif')
par(mgp=c(4,1.5,0))
par(mar = c(6, 7, 3, 2))
x_lab <- 'CR'
y_lab <- 'PR'
c_lab <- c_1

scatter2D(tmp.R1.R2[,'CR'],tmp.R1.R2[,'PR'],
          colvar = tmp.R1.R2[,'c1'],
          cex= 3/((tmp.R1.R2[,'c1'] + tmp.R1.R2[,'c2'])/2 + 1),
          pch= 19,
          col = gray.colors(6,start = 0.1,end=0.9),
          cex.lab = cex.lab, cex.axis = cex.axis, colkey = list(cex.clab = cex.clab, cex.axis = cex.axis),
          clab = c_lab,xlab = x_lab, ylab = y_lab)
legend('bottomleft',legend = c(c_12,'0','1','2','3','4','5'), bty = 'n',
       pt.cex = c(0,sort(as.numeric(levels(as.factor(3/((tmp.R1.R2[,'c1'] + tmp.R1.R2[,'c2'])/2 + 1)))),decreasing = TRUE)),
       cex = legend_cex,
       pch = 19)

dev.off()


#CR vs RP fix R1,R2
pdf("G:/My Drive/research topics/2017 SRDT & warranty/2020JQT/manuscript/figures/Tradeoff_real_CRRP_c.pdf",
    width=10, height=8)
par(family='serif')
par(mgp=c(4,1.5,0))
par(mar = c(6, 7, 3, 2))

x_lab <- 'CR'
y_lab <- 'RP'
c_lab <- c_1
scatter2D(tmp.R1.R2[,'CR'],tmp.R1.R2[,'RP'],
          colvar = tmp.R1.R2[,'c1'],
          cex= 3/((tmp.R1.R2[,'c1'] + tmp.R1.R2[,'c2'])/2 + 1),
          pch= 19,
          col = gray.colors(6,start = 0.1,end=0.9),
          cex.lab = cex.lab, cex.axis = cex.axis, colkey = list(cex.clab = cex.clab, cex.axis = cex.axis),
          clab = c_lab,xlab = x_lab, ylab = y_lab)
legend('bottomleft',legend = c(c_12,'0','1','2','3','4','5'), bty = 'n',
       pt.cex = c(0,sort(as.numeric(levels(as.factor(3/((tmp.R1.R2[,'c1'] + tmp.R1.R2[,'c2'])/2 + 1)))),decreasing = TRUE)),
       cex = legend_cex,
       pch = 19)

dev.off()


