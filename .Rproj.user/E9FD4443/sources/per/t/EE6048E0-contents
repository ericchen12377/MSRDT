#CR vs PR fix c1,c2
pdf("G:/My Drive/research topics/2017 SRDT & warranty/2020JQT/manuscript/figures/Tradeoff_real_CRPR_R.pdf",
    width=10, height=8)
par(family='serif')
par(mgp=c(4,1.5,0))
par(mar = c(6, 7, 3, 4))

x_lab <- 'CR'
y_lab <- 'PR'
c_lab <- R_1
scatter2D(tmp.c1.c2[,'CR'],tmp.c1.c2[,'PR'],
          colvar = tmp.c1.c2[,'R1'],
          cex = (as.integer(as.factor(tmp.c1.c2[,'R2']))+1) / 3,
          pch = 19,
          col = gray.colors(10,start = 0.9,end=0.1),
          cex.lab = cex.lab, cex.axis = cex.axis, colkey = list(cex.clab = cex.clab, cex.axis = cex.axis),
          clab = c_lab,xlab = x_lab, ylab = y_lab)
legend('topright',
       legend = c(R_2,'0.8','0.82','0.84','0.86','0.88','0.9'), bty = 'n',
       pt.cex = c(0,sort(as.numeric(levels(as.factor((as.integer(as.factor(tmp.c1.c2[,'R2']))+1) / 3))),decreasing = FALSE)),
       cex = legend_cex,
       pch = 19)
dev.off()


#CR vs RP fix c1,c2
